{
  "name": "jukeboxvr-electron",
  "version": "1.1.0",
  "description": "Yo!",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "watch-dev": "webpack --watch --config _builders/webpack.dev.js",
    "publish": "build -p always"
  },
  "repository": "https://github.com/davidmars/jukeboxvr-electron",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^3.0.4",
    "electron-builder": "^20.28.4",
    "clean-webpack-plugin": "^0.1.18",
    "copy-webpack-plugin": "^4.4.1",
    "css-loader": "^0.28.9",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "grunt": "^1.0.3",
    "grunt-cli": "^1.2.0",
    "grunt-contrib-clean": "^1.1.0",
    "grunt-svg-symbols": "^1.0.0",
    "grunt-svgmin": "^5.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^2.30.1",
    "install": "^0.10.4",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "less-plugin-autoprefix": "^1.5.1",
    "less-plugin-lists": "^1.1.1",
    "load-grunt-tasks": "^3.5.2",
    "md5-file": "^3.2.3",
    "node-sass": "^4.9.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "svg-inline-loader": "^0.8.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.12.0",
    "webpack-cli": "^2.1.5",
    "webpack-merge": "^4.1.3"
  },
  "build": {
    "appId": "com.github.davidmars.jukeboxvr-electron",
    "win": {
      "publish": [
        "github"
      ],
      "asar": true,
      "extraFiles": [
        "jukebox/*.html"
      ],
      "target": [
        {
          "target": "nsis",
          "createDesktopShortcut":"always",
          "createStartMenuShortcut":"always",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    }
  },
  "dependencies": {
    "electron-log": "^2.2.17",
    "electron-updater": "^3.1.2",
    "event-emitter-es6": "^1.1.5",
    "jquery": "^3.3.1"
  }
}
